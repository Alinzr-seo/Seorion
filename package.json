{
	"name": "seorion",
	"version": "0.0.3",
	"description": "SEO-focused route definition system for React apps using TanStack Router + Helmet + full SEO automation.",
	"keywords": [
		"seo",
		"router",
		"react",
		"sitemap",
		"helmet",
		"typescript",
		"tanstack",
		"vite",
		"ssr"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.cjs"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.tsx --dts --format esm,cjs",
		"prepublishOnly": "npm run build"
	},
	"author": {
		"name": "Alinzr",
		"url": "https://github.com/Alinzr-seo"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Alinzr-seo/Seorion.git"
	},
	"bugs": {
		"url": "https://github.com/Alinzr-seo/Seorion/issues"
	},
	"homepage": "https://github.com/Alinzr-seo/Seorion#readme",
	"license": "MIT",
	"peerDependencies": {
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0",
		"react-router-dom": ">=6.0.0",
		"react-helmet-async": ">=1.3.0"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.76.1",
		"@tanstack/react-router": "^1.120.10",
		"path-to-regexp": "^6.2.1"
	},
	"devDependencies": {
		"@types/node": "^20.0.0",
		"@types/react": "^18.2.41",
		"@types/react-dom": "^18.2.19",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3"
	}
}
